{
    "env": {
        "test": {
            "presets": [
                "@babel/preset-env"
            ]
        },
        "development": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "modules": false
                    }
                ]
            ],
            "plugins": [
                "babel-plugin-transform-typescript-metadata",
                "@babel/plugin-external-helpers"
            ]
        }
    }
}
